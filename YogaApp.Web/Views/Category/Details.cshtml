@model CategoryDetailViewModel

<h2>@Model.CategoryName</h2>
<p><strong>Category Description:</strong> @Model.CategoryDescription</p>
<p><strong>Poses in this category:</strong></p>
<ul>
    @foreach (var pose in Model.PoseLink)
    {
        <li><a asp-action="Details" asp-controller="Pose" asp-route-id="@pose.PoseId">
                <img src="@pose.ThumbnailSvg" alt="@pose.PoseName" height="40px">
            @pose.PoseName
        </a></li>
    }
</ul>